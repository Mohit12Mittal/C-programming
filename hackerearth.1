#include <stdio.h>
#include<string.h>
int main(){
	char str[100];
	scanf("%s",str);
	int count=0,sum=0;
	int len=strlen(str);
	printf("%d ",len);
	for(int i=0;i<len;i++)
	{
		if(str[i]=='z')
		{
			count++;
		}
		else if(str[i]=='o')
		{
			sum++;
		}
	}
	printf("%d %d",count,sum);
	if(count==(sum*2))
	{
		printf("Yes");
	}
	else
	{
		printf("No");
	}
return 0;
}
