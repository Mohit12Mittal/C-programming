#include<stdio.h>
int main()
{
    int num,evensum=0,oddsum=0;
    printf("Enter num");
    scanf("%d",&num);
    int a[num];
    printf("Enter the value");
    for(int i=0;i<num;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int j=0;j<num;j++)
    {
        if(a[j]%2==0)
        {
         evensum=evensum+a[j];
        }
        else
        {
            oddsum=oddsum+a[j];
        }
    }
    printf("number of even num is=%d\n",evensum);
    printf("number of odd num is=%d\n",oddsum);
    return 0;
    
}
