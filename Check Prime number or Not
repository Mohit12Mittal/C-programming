#include <stdio.h>

int main()
{
    int num,res=0;
    printf("Enter the number to check whether a given number is prime or not");
    scanf("%d",&num);
    for(int i=2;i<=num/2;i++)
    {
        if(num%i==0)
        {
            res=1;
        }
    }
    if(res==1)
    {
        printf("Given number is not Prime");
    }
    else
    {
        printf("Number is prime");
    }

    return 0;
}
