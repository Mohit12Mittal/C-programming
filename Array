#include<stdio.h>
int main()
{
    int num;
    printf("how many Element you want to search in array");
    scanf("%d",&num);
    int A[num];
    for(int i=0;i<num;i++)
    {
        scanf("%d",&A[i]);
    }
    int max=A[0];
    int min=A[0];
    for(int i=0;i<num;i++)
    {
        if(max<A[i])
        {
            max=A[i];
        }
    }
    printf("Maximum Element is :%d\n",max);
    for(int i=0;i<num;i++)
    {
        if(min>A[i])
        {
            min=A[i];
        }
    }
    printf("Minimun Element in this Group is:%d\n",min);
    
    return 0;
}
