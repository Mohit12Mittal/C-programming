#include<stdio.h>
int main()
{
    int num;
    printf("Enter number you want to check to be prime");
    scanf("%d",&num);
    int isprime=1;
    for(int i=2;i*i<num;i++)
    {
        if(num%i==0)
        {
            isprime=0;
        }
    }
    if(isprime)
    {
        printf("Number is prime");
    }
    
        else
        {
            printf("Number is not prime");
        }
        printf("\n");
    return 0;
}
