#include<stdio.h>
int main()
{
    int num,res;
    scanf("%d",&num);
    int arr[num];
    for(int i=0;i<num;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<num-1;i++)
    {
        if(arr[i+1]-arr[i]>0)
        {
            res=arr[i+1]-arr[i];
            printf("%d\n",res);
        }
        else
        {
         res=0;
         printf("%d\n",res);
        }
       
    }
    return 0;
    
}
